nt(\"It won't fight back this turn\")\n","\n","    return 1\n","\n","  elif action == \"Parry\":\n","    print(\"The knight parried the attack. He didn't receive any damage\")\n","\n","    return 1\n","\n","  elif action == \"Spare\":\n","    print(\"The knight pleas for mercy\")\n","\n","    a = rand.randint(0, 1)\n","\n","    if a == 0:\n","      print(\"But the dragon refused\")\n","    else:\n","      print(\"The dragon let the knight flee\")\n","      return 0\n","\n","    return 1\n","\n","  else:\n","    print(\"The knight stood and did nothing. He was burned to crisp...\")\n","    print(\"Game over\")\n","    return 0\n","\n","\n","print(\"The knight got lost in the woods to his own surprise.\")\n","print(\"The places seemes unknown to him, so he decided to wander around\", end=\" \")\n","print(\"just to see if he can find a way out.\\nBut unfortunately he\", end=\" \")\n","print(\"stumbled upon a dragon lair.\\nThe dragon was awake and wasn't happy\", end=\" \")\n","print(\"about the knight's presence at it's place.\\nSo the knight was forced\", end=\" \")\n","print(\"into a fight...\\n\")\n","\n","while True:\n","  print(\"The fight begins\\n\")\n","\n","  if control() == 0: break\n","\n","  else:\n","    if character['health'] <= 0:\n","      print(\"The knight was defeated\")\n","      break\n","\n","    if dragon['health'] <= 0:\n","      print(\"The dragon was defeated\")\n","      break\n","\n","\n","\n"],"metadata":{"id":"LCcxp5Q0srjY"},"execution_count":null,"outputs":[]},{"cell_type":"markdown","source":["# Задание 3\n"],"metadata":{"id":"N8oFiEXol3e2"}},{"cell_type":"markdown","source":["Цель - создать менеджера команды Pokémon, который позволит пользователям:\n","\n","- Добавлять покемонов в свою команду. (если такого покемона еще нет в команде)\n","- Удалять покемонов из их команды.\n","- Просматривать подробную информацию обо всех покемонах в команде.\n","- Находить покемона по имени.\n","- Устраивать тренировочный бой между двумя покемонами\n","\n","Для данной задачи используйте: https://pokeapi.co/\n"],"metadata":{"id":"9p4WQG_Vl7wB"}},{"cell_type":"code","source":["import requests as req\n","import random\n","\n","\n","data = req.get(\"https://pokeapi.co/api/v2/pokemon/?limit=1303\").json()\n","listOfPoke = data['results'][:1025]\n","team = []\n","\n","\n","def add():\n","  name = input(\"Name of the pokemon: \")\n","  ind = 0\n","\n","  for i in range(1025):\n","    if listOfPoke[i]['name'] == name.lower():\n","      ind = i+1\n","      break\n","\n","  pokeData = req.get(f\"https://pokeapi.co/api/v2/pokemon/{ind}/\").json()\n","\n","  height = pokeData['height']\n","  weight = pokeData['weight']\n","  typ = list(pokeData['types'][i]['type']['name']\\\n","            for i in range(len(pokeData['types'])))\n","  abils = []\n","  for i in range(len(pokeData['abilities'])):\n","    abils.append(pokeData['abilities'][i]['ability']['name'])\n","\n","  return {'name': name, 'ht': height, 'wt': weight, 'type': typ, 'abils': abils}\n","\n","\n","def delete(team):\n","  name = input(\"Name of the pokemon: \")\n","\n","  for i in range(len(team)):\n","    if team[i]['name'] == name:\n","      team = team[:i] + team[i+1:]\n","      break\n","\n","  return team\n","\n","\n","def infoByName(name, team):\n","  name = input(\"Name of the pokemon: \")\n","\n","  for i in range(len(team)):\n","    if team[i]['name'] == name:\n","      print(f\"Name: {team[i]['name']}\")\n","      print(f\"Height: {team[i]['ht']}\")\n","      print(f\"Weight: {team[i]['wt']}\")\n","      print(f\"Type: {team[i]['typ']}\")\n","      print(f\"Abilities: {team[i]['abils']}\")\n","      break\n","\n","\n","def info(team):\n","  for i in range(len(team)):\n","    print(f\"Name: {team[i]['name']}\", end=\"; \")\n","    print(f\"Height: {team[i]['ht']}\", end=\"; \")\n","    print(f\"Weight: {team[i]['wt']}\", end=\"; \")\n","    print(f\"Type: {team[i]['type']}\", end=\"; \")\n","    print(f\"Abilities: {team[i]['abils']}\")\n","\n","\n","def fight(poke1, poke2):\n","  win = [(\"Rock\", \"Paper\"), (\"Scissors\", \"Rock\"), (\"Paper\", \"Scissors\")]\n","  correct = [\"Rock\", \"Paper\", \"Scissors\"]\n","  score1, score2 = 0, 0\n","\n","  while True:\n","    c = correct[random.randint(0, 2)]\n","    cc = correct[random.randint(0, 2)]\n","\n","    print(f\"{poke1['name']}: \" + cc)\n","    print(f\"{poke2['name']}: \" + c)\n","\n","    if c == cc:\n","      print(\"Draw\\n\")\n","\n","    else:\n","      for i in win:\n","        if (cc, c) == i:\n","          print(f\"{poke2['name']} won!\\n\")\n","          score1 += 1\n","          break\n","\n","        if (c, cc) == i:\n","          print(f\"{poke1['name']} won!\\n\")\n","          score2 += 1\n","          break\n","\n","    if score1 == 3:\n","      print(f\"{poke2['name']} won (3 wins)!\\n\")\n","      break\n","\n","    if score2 == 3:\n","      print(f\"{poke2['name']} won (3 wins)!\\n\")\n","      break\n","\n","\n","print(\"Enter one of the codes to use the following commands:\")\n","print(\"1 - add a pokemon to your team\")\n","print(\"2 - delete a pokemon from your team\")\n","print(\"3 - get info about your team\")\n","print(\"4 - get info about specific pokemon\")\n","print(\"5 - set up a 'fight' between 2 pokemons\")\n","print(\"6 - shut down the app\")\n","\n","\n","while True:\n","  cmd_code = int(input(\"Command code: \"))\n","\n","  if cmd_code == 1:\n","    team += [add()]\n","    print(\"Pokemon added\")\n","\n","  elif cmd_code == 2:\n","    name = input(\"Enter the name of the pokemon you want to remove: \")\n","    delete(name)\n","\n","    print(\"Pokemon deleted\")\n","\n","  elif cmd_code == 3: info(team)\n","\n","  elif cmd_code == 4:\n","    name = input(\"Enter the name of the pokemon you want to know about: \")\n","    infoByName(team, name)\n","\n","  elif cmd_code == 5:\n","    if len(team) >= 2:\n","      name1 = input(\"Enter the name of the pokemon: \")\n","      name2 = input(\"Enter the name of another pokemon: 